<view class="top-search seach-real">
    <image class="top-search-icon" mode="aspectFill" src="../../style/images/search.png"></image>
    <input class="top-search-ipt" placeholder-class="top-search-ipt-h" placeholder="客户 / 拣货任务 / 路线" auto-focus="true" />
    <view class="search-real-cancel">
        <text bindtap="cancel">取消</text>
    </view>
</view>

<scroll-view scroll-y class="scroll-view-container">
    <view class="rs-item-container" wx:for="{{tdata}}" wx:key="item.value">
        <text>{{item.label}}</text>
    </view>
  </scroll-view>